            xmlns="http://javafx.com/javafx/8.0.192" fx:controller="Game.frontend.MainMenu.MainMenuController">
    <stylesheets>
        <URL value="@MainMenu.css"/>
    </stylesheets>
    <Button id="NewGame" fx:id="NewGameButton" layoutX="822.0" layoutY="456.0" mnemonicParsing="false"
            onMouseClicked="#newGameClick" prefHeight="66.0" prefWidth="190.0" text="New Game" textFill="WHITE"/>
    <Button id="ResumeGame" fx:id="ResumeButton" layoutX="1088.0" layoutY="456.0" mnemonicParsing="false"
            onMouseClicked="#resumeClick" prefHeight="66.0" prefWidth="190.0" text="Resume Game" textFill="WHITE"/>
    <Button id="Help" fx:id="HelpButton" layoutX="822.0" layoutY="559.0" mnemonicParsing="false"
            onMouseClicked="#helpClick" prefHeight="66.0" prefWidth="190.0" text="Help" textFill="WHITE"/>
    <Button id="Exit" fx:id="ExitButton" layoutX="1088.0" layoutY="559.0" mnemonicParsing="false"
            onMouseClicked="#exitClick" prefHeight="66.0" prefWidth="190.0" text="Exit" textFill="WHITE"/>
