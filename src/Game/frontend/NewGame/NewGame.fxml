<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="pane" pickOnBounds="false" prefHeight="710"
            prefWidth="1300.0" stylesheets="@NewGame.css" xmlns="http://javafx.com/javafx/8.0.192"
            fx:controller="Game.frontend.NewGame.NewGameController">
    <TextField fx:id="usernameTextBox" layoutX="338.0" layoutY="161.0" onKeyReleased="#usernameKeyRelease"
               prefHeight="94.0" prefWidth="623.0" promptText="Enter username">
        <font>
            <Font name="Comic Sans MS" size="21.0"/>
        </font>
    </TextField>
    <Button id="addUser" fx:id="addUserButton" layoutX="474.0" layoutY="327.0" mnemonicParsing="false"
            onMouseClicked="#newUserClick" prefHeight="56.0" prefWidth="129.0" text="Add User"/>
    <Button id="mainMenu" layoutX="716.0" layoutY="327.0" mnemonicParsing="false" onMouseClicked="#mainMenuClick"
            prefHeight="56.0" prefWidth="129.0" text="Main Menu"/>
</AnchorPane>
